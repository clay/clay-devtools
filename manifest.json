{
  "name": "Clay Slip",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["highlight.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "name": "Activate Clay Slip",
    "default_icon": "clay.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
